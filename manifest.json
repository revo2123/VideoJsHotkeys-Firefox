{
  "manifest_version": 2,
  "name": "Video Hotkeys",
  "version": "1.0",
  "description": "Adds potentially missing hotkeys for VideoJs Plugin",
  "icons": {
    "48": "icons/48.png"
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_title": "Video Hotkeys",
    "default_popup": "popup/index.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["addHotkeys.js"]
    }
  ]
}